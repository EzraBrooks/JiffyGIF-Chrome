{
  "manifest_version": 2,

  "name": "JiffyGIF",
  "description": "This extension replaces .GIF images with their HTML5 video equivalent via gfycat.",
  "version": "0.1",
  "background":{
    "scripts":["background.js"],
    "persistent": false
  },
  "permissions": [
    "<all_urls>",
    "webNavigation"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}