{
  "manifest_version": 2,

  "name": "JiffyGIF",
  "description": "This extension replaces .GIF images with their HTML5 video equivalent via gfycat.",
  "version": "0.3",
  "background":{
    "scripts": ["background.js"]
  },
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ]
}